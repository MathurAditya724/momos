{
  "name": "@momos/inspector",
  "productName": "Momos Inspector",
  "description": "MCP inspector for debugging and inspecting MCP servers.",
  "version": "0.2.0",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "type": "module",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "sandbox",
    "inspector",
    "debugging",
    "debugger",
    "tools",
    "server"
  ],
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron-vite dev --entry ./dist-electron/main/index.js",
    "build": "vite build --mode client && vite build",
    "build:electron": "electron-vite build"
  },
  "homepage": "https://github.com/rhinobase/momos",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinobase/momos.git",
    "directory": "packages/inspector"
  },
  "bugs": {
    "url": "https://github.com/rhinobase/momos/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/main.d.ts",
        "default": "./dist/client/main.js"
      },
      "require": {
        "types": "./dist/client/main.d.cts",
        "default": "./dist/client/main.cjs"
      }
    }
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.23.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^22.15.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "electron": "^39.1.2",
    "electron-vite": "^4.0.1",
    "rollup-plugin-sourcemaps2": "^0.5.4",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.11.38",
    "@assistant-ui/react-ai-sdk": "^1.1.10",
    "@assistant-ui/react-markdown": "^0.11.4",
    "@hono/mcp": "^0.1.5",
    "@hono/node-server": "^1.19.6",
    "@hono/standard-validator": "^0.1.5",
    "@hookform/resolvers": "^5.2.2",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@shikijs/langs": "^3.15.0",
    "@shikijs/themes": "^3.15.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@tanstack/react-query": "^5.90.9",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "hono": "^4.10.4",
    "hono-openapi": "^1.1.1",
    "lucide-react": "^0.553.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-resizable-panels": "^3.0.6",
    "react-router": "^7.9.6",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.15.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  }
}
